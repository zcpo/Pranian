{
  "rules": {
    ".read": true,
    ".write": "auth != null",
    "feed_items": {
      ".indexOn": ["createdAt"],
      "$postId": {
        "userId": {
          ".validate": "newData.val() === auth.uid"
        },
        "createdAt": {
          ".validate": "newData.val() <= now"
        },
        ".write": "data.child('userId').val() === auth.uid || root.child('admins').child(auth.uid).exists()"
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "admins": {
      "mcculloughkenya65": {
        ".write": false,
        ".read": true
      }
    }
  }
}
