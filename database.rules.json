
{
  "rules": {
    "feed_items": {
      ".read": true,
      ".write": "auth != null",
      ".indexOn": ["createdAt"],
      "$postId": {
        "userId": {
          ".validate": "newData.val() === auth.uid"
        },
         "createdAt": {
           ".validate": "newData.val() <= now"
         }
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
